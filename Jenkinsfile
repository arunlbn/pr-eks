pipeline {
  agent any
  stages {
    stage('Checkout') {
      parallel {
        stage('Checkout') {
          steps {
            sh 'pwd'
          }
        }

        stage('Setup Terraform ENV') {
          steps {
            sh 'pwd'
          }
        }

        stage('TF Plan') {
          steps {
            sh 'pwd'
          }
        }

        stage('TF Apply') {
          steps {
            sh 'pwd'
          }
        }

      }
    }

  }
}